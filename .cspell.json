{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "words": [
    "bunup",
    "choco",
    "coreutils",
    "direnv",
    "dotenv",
    "dsanders",
    "esac",
    "fkirc",
    "gitmoji",
    "globber",
    "jscoverage",
    "jspm",
    "liblaf",
    "nuxt",
    "octocat",
    "orhun",
    "pids",
    "pipx",
    "topo",
    "unskippable",
    "vuepress",
